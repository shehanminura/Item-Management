<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="160.0" prefWidth="340.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controller.deleteItem">
    <children>
        <Label fx:id="lblHello" alignment="CENTER" contentDisplay="CENTER" layoutX="14.0" layoutY="14.0" prefHeight="48.0" prefWidth="310.0" text="Delete Item" textAlignment="CENTER" textFill="#7a044d" textOverrun="CENTER_ELLIPSIS">
            <font>
                <Font name="Comic Sans MS Bold" size="31.0" />
            </font>
        </Label>
        <TextField fx:id="txtId" layoutX="83.0" layoutY="60.0" prefHeight="41.0" prefWidth="171.0" promptText="id">
            <font>
                <Font size="20.0" />
            </font>
        </TextField>
        <Button fx:id="btnViewTable" layoutX="110.0" layoutY="115.0" mnemonicParsing="false" onAction="#btnViewTableOnAction" prefHeight="25.0" prefWidth="117.0" style="-fx-background-color: #5c3e60;" text="Delete " textFill="WHITE">
            <font>
                <Font name="Comic Sans MS" size="12.0" />
            </font>
            <opaqueInsets>
                <Insets />
            </opaqueInsets>
        </Button>
    </children>
